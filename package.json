{
  "name": "pdf-shop",
  "version": "1.1.0",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "scripts": {
    "start": "serverless offline start",
    "test": "NODE_ENV=test node_modules/.bin/mocha --recursive --require babel-core/register",
    "test:watch": "NODE_ENV=test nodemon --exec 'node_modules/.bin/mocha --recursive --require babel-core/register -R min --exit'",
    "lint": "node_modules/.bin/eslint .",
    "run:createPdf": "node_modules/.bin/babel-node run/createPdf.js"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-promise": "^4.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "serverless-offline": "^3.25.6",
    "serverless-webpack": "^5.1.0",
    "webpack": "^4.16.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.304.0",
    "babel-runtime": "^6.26.0",
    "html-pdf": "^2.2.0",
    "pdfjs-dist": "^2.0.489",
    "shortid": "^2.2.13",
    "source-map-support": "^0.4.18",
    "uuid": "^3.3.2"
  }
}
